{% extends 'main/index.html.twig' %}

{% block title %}Liste des Sessions : {% endblock %}

{% block content %}
    {% for user in users_session %}
        <div>
            {{ user.username }}
            <a href="{{ path('user_history', {'id': user.id}) }}">Voir son qcm</a>
            Discord : <a href="discord://discord.com/users/{{ user.discordId }}">{{ user.usernameDiscord }}</a>
        </div>
    {% endfor %}

{% endblock %}